<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <artifactId>watch-cat</artifactId>
    <groupId>fun.tusi</groupId>
    <version>1.0.2-sb3</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <artifactId>watch-cat-vaildator</artifactId>
  <version>${watch-cat-vaildator.version}</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.4.1</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <artifactSet>
                <includes>
                  <include>fun.tusi:watch-cat-utils</include>
                </includes>
              </artifactSet>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>org.hibernate.validator</groupId>
      <artifactId>hibernate-validator</artifactId>
      <version>8.0.0.Final</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>fun.tusi</groupId>
      <artifactId>watch-cat-utils</artifactId>
      <version>1.0.0-sb3</version>
      <scope>provided</scope>
    </dependency>
  </dependencies>
  <properties>
    <java.version>17</java.version>
  </properties>
</project>
